#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    
    int smallest = arr[0];
    for(int i = 1; i < n; i++) {
        if(arr[i] < smallest)
            smallest = arr[i];
    }

   
    int second_smallest;
    int i;
    for(i = 0; i < n; i++) {
        if(arr[i] > smallest) {
            second_smallest = arr[i];
            break;
        }
    }

   
    if(i == n) {
        printf("-1");
        return 0;
    }

    
    for(i = i + 1; i < n; i++) {
        if(arr[i] > smallest && arr[i] < second_smallest) {
            second_smallest = arr[i];
        }
    }

    printf("%d", second_smallest);
    return 0;
}
