#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N-1];
    int sum = 0;

    for (int i = 0; i < N - 1; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    int expected_sum = N * (N + 1) / 2;
    int missing = expected_sum - sum;

    printf("%d", missing);
    return 0;
}
