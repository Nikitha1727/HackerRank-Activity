#include <stdio.h>

// Function to left rotate array by one position
void leftRotateByOne(int arr[], int n) {
    if (n <= 1) return;   // No rotation needed

    int first = arr[0];   // Store first element

    // Shift elements to the left
    for (int i = 0; i < n - 1; i++) {
        arr[i] = arr[i + 1];
    }

    arr[n - 1] = first;   // Place first element at the end
}

int main() {
    int n;

    // Read size of array
    scanf("%d", &n);

    int arr[n];

    // Read array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Perform left rotation
    leftRotateByOne(arr, n);

    // Print rotated array
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
