#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%lld", &arr[i]);
    }

    int ascending = 1, descending = 1;

    for (int i = 0; i < n - 1; i++) {
        if (arr[i] > arr[i + 1]) {
            ascending = 0;
        }
        if (arr[i] < arr[i + 1]) {
            descending = 0;
        }
    }

    if (ascending) {
        printf("Ascending");
    } else if (descending) {
        printf("Descending");
    } else {
        printf("Not Sorted");
    }

    return 0;
}
