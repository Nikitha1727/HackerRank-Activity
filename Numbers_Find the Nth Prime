#include <stdio.h>

int main() {
    int n, count = 0;
    scanf("%d", &n);

    for (int num = 2;  ;num++) {
        int Prime = 1;

        for (int i = 2; i * i <= num; i++) {
            if (num % i == 0) {
                Prime = 0;
                break;
            }
        }

        if (Prime) {
            count++;
            if (count == n) {
                printf("%d", num);
                break;
            }
        }
    }

    return 0;
}
