#include <stdio.h>

void sortArray(int arr[], int n) {
    int count7 = 0, count8 = 0, count9 = 0;

    // Count occurrences
    for (int i = 0; i < n; i++) {
        if (arr[i] == 7) count7++;
        else if (arr[i] == 8) count8++;
        else count9++;
    }

    // Overwrite array in-place
    int index = 0;

    for (int i = 0; i < count7; i++)
        arr[index++] = 7;

    for (int i = 0; i < count8; i++)
        arr[index++] = 8;

    for (int i = 0; i < count9; i++)
        arr[index++] = 9;
}

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    sortArray(arr, n);

    // Print sorted array
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
