#include <stdio.h>
#include <limits.h>

int main() {
    int n;
    scanf("%d", &n);

    int x;
    int smallest = INT_MAX, second_smallest = INT_MAX;
    int largest = INT_MIN, second_largest = INT_MIN;

    for (int i = 0; i < n; i++) {
        scanf("%d", &x);

        if (x < smallest) {
            second_smallest = smallest;
            smallest = x;
        } else if (x < second_smallest) {
            second_smallest = x;
        }

        if (x > largest) {
            second_largest = largest;
            largest = x;
        } else if (x > second_largest) {
            second_largest = x;
        }
    }

    long long left = (long long)second_largest * smallest;
    long long right = (long long)second_smallest + largest;

    if (left > right)
        printf("True");
    else
        printf("False");

    return 0;
}
