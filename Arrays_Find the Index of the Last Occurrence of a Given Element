#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    long long arr[N];

    for (int i = 0; i < N; i++) {
        scanf("%lld", &arr[i]);
    }

    long long K;
    scanf("%lld", &K);

    int index = -1;

    // Traverse from end to find last occurrence
    for (int i = N - 1; i >= 0; i--) {
        if (arr[i] == K) {
            index = i;
            break;
        }
    }

    printf("%d", index);

    return 0;
}
