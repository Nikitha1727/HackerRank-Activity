#include <stdio.h>

void staircase(int n) {
    for(int i = 1; i <= n; i++) {
        // Print spaces
        for(int j = 1; j <= n - i; j++) {
            printf(" ");
        }
        // Print hashes
        for(int k = 1; k <= i; k++) {
            printf("#");
        }
        printf("\n");
    }
}

int main() {
    int n;
    scanf("%d", &n);
    staircase(n);
    return 0;
}
